<launch>
    <!-- start the robot simulation grasp environment-->
    <include file="$(find grasp_icp)/launch/UR_robot_sim.launch"> </include> 
    
    <!-- Load  pose transformation and registration server-->
    <include file="$(find pose_transformation)/launch/pose_transform.launch"> </include> 
    <include file="$(find halcon_package)/launch/registration.launch"> </include> 

    <!-- start the kinect_obtain_pcd function-->
    <node pkg="grasp_icp" type="obtain_pcd.py" name="kinect_sim" output="screen"> </node>
    
    <!-- start robot grasping main process-->
    <node pkg="grasp_icp" type="main_process.py" name="Grasping" output="screen"> </node>

</launch>
